{
    "version": 2,
    "builds": [
        {
            "src": "backend/app/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "prototype/index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/app/main.py"
        },
        {
            "src": "/docs",
            "dest": "/backend/app/main.py"
        },
        {
            "src": "/openapi.json",
            "dest": "/backend/app/main.py"
        },
        {
            "src": "/",
            "dest": "/prototype/index.html"
        }
    ]
}